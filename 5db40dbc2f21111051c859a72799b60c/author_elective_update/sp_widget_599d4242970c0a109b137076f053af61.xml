        <client_script><![CDATA[api.controller = function() {
  // widget controller
  var c = this;
	console.log("data", c.data);
	chatConfig = KoreSDK.chatConfig;
	console.log("chatConfig", chatConfig);
  chatConfig.botOptions.JWTUrl =c.data["JWT URL"];
  chatConfig.botOptions.botInfo._id = c.data["Bot ID"];
  chatConfig.botOptions.botInfo.name = "ITSM Bot";
	chatConfig.botOptions.botInfo.customData = {
		"rtmType": "web",
		itsmUserId:c.data.userId
	};
	
	 
  chatConfig.botOptions.clientId = c.data["Client ID"];
  chatConfig.botOptions.clientSecret = 'ej+5mnvEkuuzuyQptYb3lGYtBa+Ucdlj0UiZ7E0KtFk=';// c.data["Client Secret"];
  //chatConfig.botOptions.assertion = c.data.jwt;
	chatConfig.botOptions.koreAPIUrl = c.data["Kore API URL"];
  chatConfig.botOptions.userIdentity = c.data.user_identity;
  chatConfig.botOptions.accountId = c.data["account id"];
	//chatConfig.botOptions.brandingAPIUrl = c.data["Kore API URL"] + "websdkthemes/" + c.data["Bot ID"] + "/activetheme";
  chatConfig.minimizeMode = true;
	KoreSDK.chatConfig = chatConfig;
	//var koreBot = koreBotChat();
	
  console.log(koreBot);
	console.log("chatConfig::",chatConfig);
	koreBot.show(chatConfig);
	 $('.openChatWindow').click(function () {
            koreBot.show(chatConfig);
        });
};]]></client_script>
